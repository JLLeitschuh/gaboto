<?xml version='1.0'?>

<!DOCTYPE rdf:RDF [
    <!ENTITY jena    'http://jena.hpl.hp.com/'>

    <!ENTITY rdf     'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
    <!ENTITY rdfs    'http://www.w3.org/2000/01/rdf-schema#'>
    <!ENTITY owl     'http://www.w3.org/2002/07/owl#'>
    <!ENTITY xsd     'http://www.w3.org/2001/XMLSchema#'>
    <!ENTITY base    '&jena;2003/04/schemagen'>
    <!ENTITY sgen    '&base;#'>
]>

<rdf:RDF
  xmlns:rdf   ="&rdf;"
  xmlns:rdfs  ="&rdfs;"
  xmlns:owl   ="&owl;"
  xmlns:sgen  ="&sgen;"
  xmlns       ="&sgen;"
  xml:base    ="&base;"
>

    <sgen:Config>
        <sgen:classTemplate rdf:datatype="&xsd;string">
    public static final String %valname%_URI = "%valuri%";
    public static final %valclass% %valname% = m_model.%valcreator%( "%valuri%" );
        </sgen:classTemplate>
        
        <sgen:propTemplate rdf:datatype="&xsd;string">
    public static final String %valname%_URI = "%valuri%";
    public static final %valclass% %valname% = m_model.%valcreator%( "%valuri%" );
        </sgen:propTemplate>
    </sgen:Config>

</rdf:RDF>
